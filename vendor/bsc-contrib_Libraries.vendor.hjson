{
  // Name of the vendored-in project
  name: "bsc-contrib_Libraries",

  // Target directory: typically equal to the name
  // All imported code is copied into this directory
  target_dir: "bsc-contrib_Libraries",

  // Git upstream source code repository
  upstream: {
    // Upstream Git repository URL. HTTPS URLs are preferred.
    url: "https://github.com/B-Lang-org/bsc-contrib",
    // Upstream revision or branch. Can be a commit hash or a branch name.
    rev: "main",
  },

  // Optional: Pick specific files or subdirectories from upstream and
  // specify where to put them.
  mapping: [
    {from: 'Libraries/AMBA_Fabrics/AXI4/AXI4_Types.bsv',
       to:           'AMBA_Fabrics/AXI4/AXI4_Types.bsv'},

    {from: 'Libraries/AMBA_Fabrics/AXI4/AXI4_BSV_RTL.bsv',
       to:           'AMBA_Fabrics/AXI4/AXI4_BSV_RTL.bsv'},

    {from: 'Libraries/Misc/Semi_FIFOF.bsv',
       to:           'Misc/Semi_FIFOF.bsv'},

    {from: 'Libraries/Misc/GetPut_Aux.bsv',
       to:           'Misc/GetPut_Aux.bsv'},

    {from: 'Libraries/Misc/EdgeFIFOFs.bsv',
       to:           'Misc/EdgeFIFOFs.bsv'},

  ]
}
